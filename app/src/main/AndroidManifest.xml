        android:supportsRtl="true"
        android:theme="@style/Theme.NHS_App">
        <activity
            android:name="uk.ac.tees.aad.icooktoo_app.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uk.ac.tees.aad.icooktoo_app.Register"
            android:exported="true" />
    </application>

</manifest>